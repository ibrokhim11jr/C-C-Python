def is_prime(num):
    if num <= 1:
        return False
    
    for i in range(2, int(num**0.5) + 1):
        if num % i == 0:
            return False
    
    return True

arr = [123, 456, 789, 101, 223, 337, 555]

print("Трехзначные числа в массиве:", end=" ")
for num in arr:
    if 100 <= num <= 999:
        print(num, end=" ")

print("\nПростые числа в массиве:", end=" ")
for num in arr:
    if is_prime(num):
        print(num, end=" ")