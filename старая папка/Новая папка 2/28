#include <stdio.h>
#include <string.h>

int main() {
    char word1[100];
    char word2[100];
    
    printf("Введите первое слово: ");
    scanf("%s", word1);
    printf("Введите второе слово: ");
    scanf("%s", word2);
    
    if (word1[0] == word2[0]) {
        printf("Первое и второе слово начинаются одинаково: %c\n", word1[0]);
    } else {
        printf("Первое и второе слово начинаются по-разному\n");
    }

    return 0;
}