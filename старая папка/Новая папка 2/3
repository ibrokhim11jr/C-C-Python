#include <stdio.h>
#include <math.h>

float calculate_function_value(float a, float b) {
    return fabs(a) + 2*sin(b)/(5.5*a);
}

int main() {
    float a, b;
    printf("Enter the value of a: ");
    scanf("%f", &a);
    printf("Enter the value of b: ");
    scanf("%f", &b);

    float result = calculate_function_value(a, b);
    printf("The value of the function for a=%f, b=%f is: %f\n", a, b, result);

    return 0;
}