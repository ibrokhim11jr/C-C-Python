#include <stdio.h>

int main() {
    int array[10] = {3, -1, 4, -2,5 , 0, -5,8, 2, -6};

    printf("Неотрицательные элементы: ");
    for (int i = 0; i < 10; i++) {
        if (array[i] >= 0) {
            printf("%d ", array[i]);
        }
    }

    printf("\nОтрицательные элементы: ");
    for (int i = 0; i < 10; i++) {
        if (array[i] < 0) {
            printf("%d ", array[i]);
        }
    }

    return 0;
}