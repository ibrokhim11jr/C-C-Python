#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    int array[15];

    // Инициализация генератора случайных чисел
    srand(time(NULL));

    printf("Массив из 15 случайных чисел: ");
    for (int i = 0; i < 15; i++) {
        array[i] = rand() % 100; // Генерация случайных чисел от 0 до 99
        printf("%d ", array[i]);
    }

    return 0;
}