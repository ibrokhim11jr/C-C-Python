#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    int array[15];

    // Инициализация генератора случайных чисел
    srand(time(NULL));

    printf("Массив из 15 случайных целых чисел в диапазоне от 0 до 10: ");
    for (int i = 0; i < 15; i++) {
        array[i] = rand() % 11; // Генерация случайного целого числа от 0 до 10
        printf("%d ", array[i]);
    }

    return 0;
}