#include <stdio.h>

int main() {
    int array[10];

    printf("Введите 10 значений для массива:\n");
    for (int i = 0; i < 10; i++) {
        scanf("%d", &array[i]);
    }

    printf("Введенный массив из 10 элементов: ");
    for (int i = 0; i < 10; i++) {
        printf("%d ", array[i]);
    }

    return 0;
}