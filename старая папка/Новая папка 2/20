#include <stdio.h>

int main() {
    int array[10] = {5, 8, 20, 31, 40, 50, 62, 73, 89, 100};

    printf("Четные элементы: ");
    for (int i = 0; i < 10; i++) {
        if (array[i] % 2 == 0) {
            printf("%d ", array[i]);
        }
    }

    printf("\nЭлементы, оканчивающиеся на 0: ");
    for (int i = 0; i < 10; i++) {
        if (array[i] % 10 == 0) {
            printf("%d ", array[i]);
        }
    }

    return 0;
}