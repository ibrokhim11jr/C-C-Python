#include <stdio.h>
#include <math.h>

int main() {
    double x, y, z;

    printf("Enter the value of x: ");
    scanf("%lf", &x);

    printf("Enter the value of y: ");
    scanf("%lf", &y);

    double numerator = x + (2 + y) / (x * x);
    double denominator = y + 1 / sqrt(x * x + 10);
    z = numerator / denominator;

    printf("The value of z is: %.2f\n", z);

    return 0;
}