#include <stdio.h>
#include <math.h>

int main() {
float x, y, q;

printf("Enter the value of x: ");
scanf("%f", &x);

printf("Enter the value of y: ");
scanf("%f", &y);

q = 7.25 * sin(x) - fabs(y); // fabs() is used to find the absolute value of y

printf("The value of the function is: %.2f\n", q);

return 0;
}