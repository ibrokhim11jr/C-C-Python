#include <stdio.h>
#include <stdbool.h>

bool isPrime(int num) {
    if (num <= 1) {
        return false;
    }
    
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0) {
            return false;
        }
    }
    
    return true;
}

int main() {
    printf("Пары чисел-близнецов до 200:\n");
    for (int i = 2; i <= 200; i++) {
        if (isPrime(i) && isPrime(i + 2)) {
            printf("%d и %d\n", i, i + 2);
        }
    }
    return 0;
}