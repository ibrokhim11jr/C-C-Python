#include <stdio.h>
#include <stdbool.h>

bool isPrime(int num) {
    if (num <= 1) {
        return false;
    }
    
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0) {
            return false;
        }
    }
    
    return true;
}

int main() {
    int arr[] = {123, 456, 789, 101, 223, 337, 555};
    int n = sizeof(arr) / sizeof(arr[0]);
    
    printf("Трехзначные числа в массиве: ");
    for (int i = 0; i < n; i++) {
        if (arr[i] >= 100 && arr[i] <= 999) {
            printf("%d ", arr[i]);
        }
    }
    
    printf("\nПростые числа в массиве: ");
    for (int i = 0; i < n; i++) {
        if (isPrime(arr[i])) {
            printf("%d ", arr[i]);
        }
    }
    
    return 0;
}